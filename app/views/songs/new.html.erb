<% provide(:title, 'New Song') %>
<h1>New Song</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@song) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= render 'song_fields', f: f %>

      <%= f.submit "Create New Song", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

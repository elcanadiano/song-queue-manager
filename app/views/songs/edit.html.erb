<% provide(:title, 'Edit Song') %>
<h1>Edit Song</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@song) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= render 'song_fields', f: f %>

      <%= f.submit "Edit Song", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

<% provide(:title, 'Bands (admin)') %>
<h1>Bands (admin)</h1>

<div class="row">
  <div class="col-md-12">
    <% if @bands.empty? %>
      <p>There are currently no bands in the system.</p>
    <% else %>
      <%= will_paginate %>

      <ul id="bands">
        <%= render @bands %>
      </ul>

      <%= will_paginate %>
    <% end %>
  </div>
</div>

<% provide(:title, 'New Soundtrack') %>
<h1>New Soundtrack</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@soundtrack) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= render 'soundtrack_fields', f: f %>

      <%= f.submit "Create New Soundtrack", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

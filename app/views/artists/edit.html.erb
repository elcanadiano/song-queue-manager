<% provide(:title, 'Edit Artist') %>
<h1>Edit Artist</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@artist) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= render 'artist_fields', f: f %>

      <%= f.submit "Edit New Artist", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
